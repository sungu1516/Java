package com.day;

public class note {
	
	String name = "LeeSungu";
	
	public static void main(String[] args) {
		
		int a = 1;
		int b = 2;
		int c = 3;
		
		int[] arr = {a, b, c};
		
		for (int i = 0; i < arr.length; i++) {
			for (int j = 0; j < arr.length; j++) {
				System.out.printf("%d <= %d", i, j + "\n");
			}
		}
		
    }	
}

