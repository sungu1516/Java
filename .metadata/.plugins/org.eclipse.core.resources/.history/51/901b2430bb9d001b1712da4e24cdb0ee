
public class Gugudan {
	
	String result = "";
	
	public void Gugudan_method() {
		
		for (int i = 2; i < 10; i++) {

			Gugudan_inner gugu = new Gugudan_inner(i); // 이 때 만들어지는 객체 gugu를 의존객체라고 한다.
			this.result += gugu;
			
			System.out.println("******************");
			

		}
	}

	public void Gugudan_method(int n) {

		for (int i = n; i < 10; i++) {

			Gugudan_inner gugu = new Gugudan_inner(i); // 이 때 만들어지는 객체 gugu를 의존객체라고 한다.
			System.out.println("******************");
		}
	}
	
	public void Gugudan_method(int n, int m) {

		for (int i = n; i < m + 1; i++) {

			Gugudan_inner gugu = new Gugudan_inner(i); // 이 때 만들어지는 객체 gugu를 의존객체라고 한다.
			System.out.println("******************");
		}
	}
}
