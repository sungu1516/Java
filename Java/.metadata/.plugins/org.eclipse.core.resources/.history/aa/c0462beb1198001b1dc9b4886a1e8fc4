package JtoJ;

public class Stringbuffer {

	public static void main(String[] args) {
		
		StringBuffer sb = new StringBuffer();
		sb.append("Hi");
		sb.append(", my name is Leesungu.");
		System.out.println(sb);
		
		/*
		 위 예문을 String 으로 한다면?
		 */
	
		String str = "";
		str += "Hi";
		str += ", my name is LeeSungu";
		System.out.println(str);
	
	}

}
