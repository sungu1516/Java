package com.day;

public class note {
	
	String name = "LeeSungu";
	
	public static void main(String[] args) {
		
		int a = 1;
		int b = 2;
		int c = 3;
		
		int[] arr = {a, b, c};
		
		for (int i = 0; i < arr.length; i++) {
			for (int j = i + 1; j < arr.length; j++) {
				System.out.printf("%d <= %d \n", arr[i], arr[j]);
			}
		}
		System.out.println("i에 대한 for문이 한 번 돌았습니다.");
    }	
}

